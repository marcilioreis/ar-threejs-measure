{
  "compilerOptions": {
    "target": "es2017",
    "jsx": "react",
    "lib": [
      "es2017",
      "es2019",
      "DOM",
      "ES6",
      "DOM.Iterable",
      "ScriptHost",
      "ES2016.Array.Include",
      "es2018.promise"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "typeRoots": [
      "node_modules/@types",
      "typings"
    ],
    "types": [
      "node"
    ]
  },
  "include": [
    "src"
  ],
  "typeAcquisition": {
    "enable": false
  }
}